{
    "maxerr": 1000,

    "indent": 2,
    "maxparams": 5,
    "maxdepth": 3,
    "maxstatements": 30,
    "maxcomplexity": 10,

    "node": true,

    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "immed": false,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true
}
